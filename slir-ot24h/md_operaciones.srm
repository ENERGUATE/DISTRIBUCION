event clicked;string sql_comando,sql_comando2
sql_comando="ALTER SESSION SET SQL_TRACE ="
sql_comando2="ALTER SESSION SET OPTIMIZER_GOAL="

if md_operaciones.m_depurar.m_optimizador.m_choose.checked then 
	sql_comando2=sql_comando2+"CHOOSE"
elseif md_operaciones.m_depurar.m_optimizador.m_rule.checked then 
		sql_comando2=sql_comando2+"RULE"
elseif md_operaciones.m_depurar.m_optimizador.m_allrows.checked then 
		sql_comando2=sql_comando2+"ALL_ROWS"
elseif md_operaciones.m_depurar.m_optimizador.m_firstrows.checked then 
		sql_comando2=sql_comando2+"FIRST_ROWS"
end if

execute immediate :sql_comando2 using SQLCA ;

if sqlca.sqlcode<>0 then
 MessageBox("Aviso","Se ha producido el siguiente error"+sqlca.sqlerrtext)
end if

if not checked then 
	sql_comando=sql_comando+ "TRUE"
	checked=true
else
	sql_comando=sql_comando+ "FALSE"
		checked=false
end if

execute immediate :sql_comando using SQLCA ;

if sqlca.sqlcode<>0 then
 MessageBox("Aviso","Se ha producido el siguiente error"+sqlca.sqlerrtext)
end if
end event

