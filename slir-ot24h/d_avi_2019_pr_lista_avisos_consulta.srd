fgnu_direccion_aviso(~"GI_AVISOS~".~"NOM_CALLE~",~"GI_AVISOS~".~"NUM_PUERTA~",~"GI_AVISOS~".~"DUPLICADOR~",~"GI_AVISOS~".~"NOM_CALLE~") direccion_aviso,
fgnu_duracion_str(~"GI_AVISOS~".~"F_ALTA~",~"GI_AVISOS~".~"FECHA_RES~") duracion_str,
fg_duracion_int( ~"GI_AVISOS~".~"F_ALTA~",~"GI_AVISOS~".~"FECHA_RES~") duracion_int,
~"GI_AVISOS~".~"OBS_IMPROC~",
