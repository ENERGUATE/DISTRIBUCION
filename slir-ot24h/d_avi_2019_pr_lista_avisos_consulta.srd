~"GI_SUMINISTROS_IMP~".~"CARGA_ES~",/*** LSH *** DDAG-845- *** 11/11/2014 */
~"GI_SUMINISTROS_IMP~".~"TIPO_CARGA_ES~",/*** LSH *** DDAG-845- *** 11/11/2014 */
~"SGD_VALOR_ESEN~".~"DESCRIPCION~" As imagen/*** LSH *** DDAG-845- *** 11/11/2014 */
FROM ~"GI_AVISOS~", ~"SGD_VALOR~", ~"SGD_CENTRO~", 
~"GI_SUMINISTROS_IMP~", ~"SGD_VALOR~" SGD_VALOR_ESEN/*** LSH *** DDAG-845- *** 11/11/2014 */
WHERE  ((~"GI_AVISOS~".~"PRIORIDAD~" = ~"SGD_VALOR~".~"CODIGO~" (+))
AND ~"SGD_VALOR~".~"CODIF~"= 'PRI') 
AND ~"SGD_CENTRO~".~"NRO_CENTRO~" = ~"GI_AVISOS~".~"NRO_MESA~" 
AND ~"SGD_CENTRO~".~"TIP_CENTRO~" = 3
and(~"GI_AVISOS~".~"NIS_RAD~" = ~"GI_SUMINISTROS_IMP~".~"NIS_RAD~" (+))/*** LSH *** DDAG-845- *** 11/11/2014 */
AND ~"SGD_VALOR_ESEN~".~"CODIF~"(+) = 'ESEN'/*** LSH *** DDAG-845- *** 11/11/2014 */
AND (~"GI_SUMINISTROS_IMP~".~"TIPO_CARGA_ES~" = ~"SGD_VALOR_ESEN~".~"VALOR~"(+))/*** LSH *** DDAG-845- *** 11/11/2014 */" retrieve.asneeded=yes  sort="f_alta D " )
