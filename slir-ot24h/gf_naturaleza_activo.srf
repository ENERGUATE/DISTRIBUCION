global function string gf_naturaleza_activo (long pl_activo);
string ls_naturaleza

//Determina si es Regional o Nacional (STR o STN)

SELECT trim(NATURALEZA)
  INTO :ls_naturaleza
  FROM BDIV10_ACTIVOS
 WHERE CODIGO = :pl_activo; 
 
 
return ls_naturaleza
end function

