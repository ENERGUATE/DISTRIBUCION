						Fgnu_Est_Inc2(SGD_INCIDENCIA.EST_ACTUAL, SGD_INCIDENCIA.ALCANCE, SGD_INCIDENCIA.TIP_INCIDENCIA),
						Fgnu_Est_Man(estado_mantenimiento),  
						Fgnu_Ten_Inc(tip_tension) ,  
						Fgnu_Tip_Inc(tip_incidencia) ,  
						Fgnu_Duracion_Str(f_deteccion,fec_resolucion),  
						1 CL_DURACION ,  
						F_Nombre_Instalacion2(SGD_INSTALACION.nom_instalacion,SGD_incidencia.dir_aviso,SGD_incidencia.alcance),   
						fgci_nombre_ot(GI_OT.NRO_OT)
FROM SGD_INSTALACION ,SGD_INCIDENCIA, GI_OT
WHERE SGD_INCIDENCIA.OT = GI_OT.NRO_OT(+) AND SGD_INSTALACION.NRO_INSTALACION = SGD_INCIDENCIA.NRO_INST_AFECTADA AND SGD_INSTALACION.BDI_JOB = 0  
						 AND TO_CHAR(SGD_incidencia.f_deteccion,'YYYYMMDD') >=  :ls_f_desde  
						 AND TO_CHAR(SGD_incidencia.f_deteccion,'YYYYMMDD') <=  :ls_f_hasta   
						 AND SGD_INCIDENCIA.EST_ACTUAL < 11   
						 AND 	SGD_INCIDENCIA.NRO_INCIDENCIA IN ( SELECT SGD_INTERRUPCION_RS.NRO_INCIDENCIA FROM SGD_INTERRUPCION_RS WHERE   
						  SGD_INTERRUPCION_RS.CABECERA_BT > 0  AND  
						SGD_INTERRUPCION_RS.NRO_INSTALACION =   
						(SELECT SGD_ACOMETIDA.INSTALACION_ORIGEN FROM SGD_ACOMETIDA, ACO_SUM   
						WHERE SGD_ACOMETIDA.CODIGO = ACO_SUM.NRO_ACOMETIDA 
								AND SGD_ACOMETIDA.BDI_JOB = 0 AND ACO_SUM.NIS_RAD =:ll_nis_rad)
						AND NVL(SGD_INTERRUPCION_RS.AFEC_PARCIAL,0) = DECODE(:li_afec_parcial, 0, NVL(SGD_INTERRUPCION_RS.AFEC_PARCIAL,0),0))   
UNION 
SELECT   
						'  ',  
						'  ',  
						'  ',   
