						SGD_INCIDENCIA.F_DETECCION,  
						SGD_INCIDENCIA.NRO_CENTRO,  
						SGD_INCIDENCIA.NRO_CMD,  
						SGD_INCIDENCIA.NRO_MESA,  
						SGD_INCIDENCIA.POT_AFECTADA,  
						SGD_INCIDENCIA.NRO_INST_AFECTADA,  
						SGD_INCIDENCIA.POT_CONTRATADA,  
						SGD_INCIDENCIA.CCLI_AFEC,  
						SGD_INCIDENCIA.TIP_TENSION,  
						SGD_INCIDENCIA.EST_ACTUAL,  
						SGD_INCIDENCIA.ESTADO_MANTENIMIENTO,  
						SGD_INCIDENCIA.H_DETECCION,  
						SGD_INCIDENCIA.NRO_INCIDENCIA,  
						SGD_INCIDENCIA.F_EST_RES,  
						SGD_INCIDENCIA.TIP_INCIDENCIA,  
						SGD_INCIDENCIA.ALCANCE,  
						' ',  
						SGD_INCIDENCIA.OBSERVACION,  
						SGD_INCIDENCIA.FEC_RESOLUCION,  
						SGD_INCIDENCIA.DIR_AVISO,  
						SGD_INSTALACION.NOM_INSTALACION NOM_INSTALACION,  
						SGD_INCIDENCIA.DESC_INCIDENCIA ,  
						'  ',  
						'  ',  
						'  ',  
						Fgnu_Est_Inc2(SGD_INCIDENCIA.EST_ACTUAL, SGD_INCIDENCIA.ALCANCE, SGD_INCIDENCIA.TIP_INCIDENCIA),
						Fgnu_Est_Man(estado_mantenimiento),  
						Fgnu_Ten_Inc(tip_tension) ,  
						Fgnu_Tip_Inc(tip_incidencia) ,  
						Fgnu_Duracion_Str(f_deteccion,fec_resolucion),  
						1 CL_DURACION ,  
						F_Nombre_Instalacion2(SGD_INSTALACION.nom_instalacion,SGD_incidencia.dir_aviso,SGD_incidencia.alcance),   
						fgci_nombre_ot(GI_OT.NRO_OT)
FROM SGD_INSTALACION ,SGD_INCIDENCIA, GI_OT, GI_AVISOS
WHERE SGD_INCIDENCIA.OT = GI_OT.NRO_OT (+) AND SGD_INSTALACION.NRO_INSTALACION = SGD_INCIDENCIA.NRO_INST_AFECTADA AND SGD_INSTALACION.BDI_JOB = 0  
	 AND TO_CHAR(SGD_incidencia.f_deteccion,'YYYYMMDD') >=  :ls_f_desde  
	 AND TO_CHAR(SGD_incidencia.f_deteccion,'YYYYMMDD') <=  :ls_f_hasta
	 AND SGD_INCIDENCIA.EST_ACTUAL < 11   
	 AND GI_AVISOS.NRO_INCIDENCIA = SGD_INCIDENCIA.NRO_INCIDENCIA
	 AND GI_AVISOS.IND_INC_MANUAL = 1
	 AND GI_AVISOS.NIS_RAD = :ll_nis_rad
	 AND SGD_INCIDENCIA.ALCANCE = fgnu_alcance_con_perdida

