table(column=(type=char(50) updatewhereclause=yes name=fase_afectada dbname="sgd_valor.fase_afectada" )
 column=(type=char(40) updatewhereclause=yes name=nom_instalacion dbname="nom_instalacion" )
 column=(type=number updatewhereclause=yes name=tipo_instalacion dbname="sgd_instalacion.tipo_instalacion" )
 column=(type=decimal(0) updatewhereclause=yes name=nro_instalacion dbname="sgd_instalacion.nro_instalacion" )
 column=(type=datetime updatewhereclause=yes name=f_actual dbname="sgd_interrupcion.f_actual" )
 retrieve="SELECT B.DESCRIPCION FASE_AFECTADA,
	NVL(SGD_INSTALACION.NOM_INSTALACION  ,SGD_INSTALACION.NRO_INSTALACION) NOM_INSTALACION,
	SGD_INSTALACION.TIPO_INSTALACION,
	SGD_INSTALACION.NRO_INSTALACION,
	SGD_INTERRUPCION.F_ACTUAL
	FROM SGD_INTERRUPCION,   		
	SGD_VALOR B ,
	SGD_INSTALACION
	WHERE ( SGD_INTERRUPCION.NRO_INCIDENCIA = :pl_nro_incid ) AND   		
	( B.CODIF(+) = 'FASE' ) AND 		
	( B.VALOR(+) = SGD_INTERRUPCION.FASE_AFECTADA ) AND
	( SGD_INSTALACION.NRO_INSTALACION = SGD_INTERRUPCION.NRO_INSTALACION) AND
	( SGD_INSTALACION.BDI_JOB = 0)
UNION SELECT 	SGD_INTERRUPCION_RS.FASE_AFECTADA,
	NVL(SGD_INSTALACION.NOM_INSTALACION  ,SGD_INSTALACION.NRO_INSTALACION) NOM_INSTALACION,
	SGD_INTERRUPCION_RS.TIPO_INSTALACION,
	SGD_INTERRUPCION_RS.NRO_INSTALACION,
	SGD_INTERRUPCION_RS.F_ACTUAL
	FROM SGD_INTERRUPCION_RS,        SGD_VALOR, SGD_INSTALACION  
	WHERE ( SGD_INTERRUPCION_RS.tipo_instalacion = sgd_valor.codigo (+)) and       
	( SGD_VALOR.CODIF = 'T_IN') AND      
	( SGD_INTERRUPCION_RS.NRO_INCIDENCIA = :pl_nro_incid)    	AND
	( SGD_INSTALACION.NRO_INSTALACION = SGD_INTERRUPCION_RS.NRO_INSTALACION) AND
	( SGD_INSTALACION.BDI_JOB = 0)" arguments=(("pl_nro_incid", number))  sort="tipo_instalacion A " )
