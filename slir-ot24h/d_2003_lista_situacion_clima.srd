 retrieve="  SELECT ~"GI_SIT_CLIMA~".~"COD_EST_CLIMA~",   
         ~"GI_SIT_CLIMA~".~"TEMP_MAX~",   
         ~"GI_SIT_CLIMA~".~"TEMP_MIN~",   
         ~"GI_SIT_CLIMA~".~"VIENTO_DIR~",   
         ~"GI_SIT_CLIMA~".~"VIENTO_VEL~",   
