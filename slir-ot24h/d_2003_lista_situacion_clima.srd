         ~"GI_SIT_CLIMA~".~"OBSERVACION~",   
         ~"GI_SIT_CLIMA~".~"FECHA~",   
         ~"GI_SIT_CLIMA~".~"NRO_CMD~"  
    FROM ~"GI_SIT_CLIMA~"  
   WHERE ~"GI_SIT_CLIMA~".~"NRO_CMD~" = DECODE(:pi_cmd,0,~"GI_SIT_CLIMA~".~"NRO_CMD~",:pi_cmd)  AND
			~"GI_SIT_CLIMA~".~"FECHA~" BETWEEN :pd_f_desde AND :pd_f_hasta	
            
