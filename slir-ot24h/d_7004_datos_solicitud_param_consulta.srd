         ~"SGD_DESCARGOS~".~"NOM_DESC~",   
         ~"SGD_DESCARGOS~".~"UBICACION~",   
         ~"SGD_DESCARGOS~".~"TEL_SOLIC~",   
         ~"SGD_VALOR~".~"DESCRIPCION~",   
         ~"SGD_DESCARGOS~".~"TRAB_TENSION~",   
         ~"SGD_DESCARGOS~".~"JEFE_TRABAJO~",   
         ~"SGD_DESCARGOS~".~"TEL_TRABAJO~",   
         ~"SGD_DESCARGOS~".~"PLANO~",   
         ~"SGD_DESCARGOS~".~"TIEMPO_REPOS~",   
         ~"SGD_DESCARGOS~".~"BRIGADA~",   
         ~"SGD_DESCARGOS~".~"SUBEST_MOVIL~"  
    FROM ~"SGD_DESCARGOS~" ,
			~"SGD_VALOR~"
	WHERE (~"SGD_DESCARGOS~".~"NRO_DESCARGO~" = :pl_nro_descargo) AND
			(~"SGD_VALOR~".~"CODIGO~" (+) = ~"SGD_DESCARGOS~".~"UNIDAD~") AND
			( ~"SGD_VALOR~".~"CODIF~" (+) = 'UNI')
