 retrieve="  SELECT ~"SGD_DESCARGOS~".~"F_INI_SOLICITADO~",   
         ~"SGD_DESCARGOS~".~"F_FIN_SOLICITADO~",   
         ~"SGD_DESCARGOS~".~"OBS_SOLICITADO~",   
         ~"SGD_DESCARGOS~".~"F_SOLICITADO~",   
