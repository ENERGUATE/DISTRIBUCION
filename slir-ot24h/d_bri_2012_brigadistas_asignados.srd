         ~"GI_BRIGADA_BRIGADISTA~".~"NRO_BRIGADISTAS~",   
         ~"GI_BRIGADA_BRIGADISTA~".~"F_FIN~",   
         ~"GI_BRIGADA_BRIGADISTA~".~"H_ALTA~",   
         ~"GI_BRIGADA_BRIGADISTA~".~"H_FIN~",   
         ~"GI_BRIGADA_BRIGADISTA~".~"ENCARGADO~",   
         NVL(~"GI_BRIGADISTA~".~"NOMBRE~",' '),   
         NVL(~"GI_BRIGADISTA~".~"APE1~", ' '),
			NVL(~"GI_BRIGADISTA~".~"APE2~", ' '),
         ~"GI_BRIGADISTA~".~"ENCARGADO~"  
    FROM ~"GI_BRIGADA_BRIGADISTA~",   
         ~"GI_BRIGADISTA~"  
   WHERE ( ~"GI_BRIGADA_BRIGADISTA~".~"NRO_BRIGADISTAS~" = ~"GI_BRIGADISTA~".~"NRO_BRIGADISTAS~" ) and  
         ( ( ~"GI_BRIGADA_BRIGADISTA~".~"NRO_BRIGADA~" = :pi_nro_brigada ) AND  
         ( ~"GI_BRIGADA_BRIGADISTA~".~"F_FIN~" is NULL ) )    
" update="GI_BRIGADA_BRIGADISTA" updatewhere=0 updatekeyinplace=no arguments=(("pi_nro_brigada", number)) )
