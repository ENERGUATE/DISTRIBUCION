 retrieve="  SELECT ~"GI_BRIGADA_BRIGADISTA~".~"NRO_BRIGADA~",   
         ~"GI_BRIGADA_BRIGADISTA~".~"F_ALTA~",   
         ~"GI_BRIGADA_BRIGADISTA~".~"USUARIO~",   
